<%= error_messages_for 'query' %>
<% if @query_bookings.valid? %>
    <% if @bookings.empty? %>
        <p class="nodata"><%= l(:label_no_data) %></p>
    <% else %>
        <%= render :partial => 'time_bookings/list_bookings', :locals => {:bookings => @bookings, :query => @query_bookings} %>
        <p class="pagination"><%= pagination_links_full @booking_pages, @booking_count, :per_page_links => false %></p>
    <% end %>
<% end %>